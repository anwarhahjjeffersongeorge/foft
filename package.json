{
  "name": "foft",
  "version": "1.2.1",
  "description": "Syntactic sugar for discontinuous (mathematical) functions.",
  "main": "src/index.js",
  "module": "src/main.js",
  "scripts": {
    "clean": "rm -rf docs test disttest .nyc_output coverage dist lib ./foftmochajs.html",
    "predoc": "clear && rm -rf docs",
    "doc": "jsdoc ./src/foft.js -c jsdoc.json -d ./docs/ -r ./README.md  --verbose -t ./node_modules/ink-docstrap/template",
    "pretest": "clear && rm -rf test coverage .nyc_output && rsync -rv src/* test",
    "test": "cross-env NODE_ENV=test nyc --require esm --reporter=text --reporter=html --show-process-tree mocha ./test/mocha/nodetestrunner.js",
    "preqtest": "clear && rm -rf test && rsync -rv src/* test",
    "qtest": "echo \"quick test\" && standard \"src/* test/* lib/index.js lib/main.js lib/foft.js\" && cross-env NODE_ENV=test mocha --require esm ./test/mocha/nodetestrunner.js",
    "pretestw": "clear && rm -rf test && rsync -rv src/* test",
    "testw": "standard \"src/* test/* lib/index.js lib/main.js lib/foft.js\" && cross-env NODE_ENV=test mocha --watch --reporter=min --require esm ./test/mocha/nodetestrunner.js",
    "preversion": "npm test && npm run doc",
    "postversion": "git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anwarhahjjeffersongeorge/foft"
  },
  "bugs": {
    "url": "https://github.com/anwarhahjjeffersongeorge/foft/issues"
  },
  "homepage": "https://github.com/anwarhahjjeffersongeorge/foft#readme",
  "keywords": [],
  "author": "Anwar Hahj Jefferson-George <anwarhahjjeffersongeorge@gmail.com>",
  "license": "Unlicense",
  "nyc": {
    "exclude": [
      "coverage/**",
      "node_modules/**",
      "test/mocha/**",
      "src/main.js"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "current node",
    "not dead"
  ],
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "chai-almost": "^1.0.1",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "dirty-chai": "^2.0.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.2",
    "minify": "^4.1.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "standard": "^12.0.1"
  }
}
