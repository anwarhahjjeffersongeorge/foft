{
  "name": "mathoftjs",
  "version": "1.0.0",
  "description": "Evaluation of math terms over specified ranges.",
  "main": "index.js",
  "module": "main.js",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "esm": "^3.0.84",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "scripts": {
    "clean": "rm -rf ./docs ./testlib ./coverage ./lib",
    "predocs": "clear && rm -rf ./docs",
    "docs": "npx jsdoc --configure .jsdoc.json --verbose",
    "pretest": "clear && rm -rf ./testlib && mkdir ./testlib && cp -r src/* ./testlib",
    "test": "NODE_ENV=test npx mocha --require @babel/register ./testlib/mocha/*.mocha.js",
    "prereport": "clear && rm -rf ./coverage && mkdir ./coverage",
    "report": "npx nyc report ./coverage",
    "prebuild": "clear && rm -rf ./lib && mkdir ./lib",
    "build": "NODE_ENV=production npx babel ./lib"
  },
  "repository": {
    "type": "git",
    "url": "pi@raspberrypi:/home/pi/GIT/mathoftjs"
  },
  "keywords": [],
  "author": "Anwar Hahj Jefferson-George <anwarhahjjeffersongeorge@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "fsevents": "^1.2.4"
  }
}
